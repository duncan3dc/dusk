{
    "name": "duncan3dc/dusk",
    "type": "library",
    "description": "Use Laravel Dusk without the full Laravel framework",
    "keywords": ["laravel", "browser", "engine", "screen", "scraping"],
    "homepage": "https://github.com/duncan3dc/dusk",
    "license": "Apache-2.0",
    "authors": [{
        "name": "Craig Duncan",
        "email": "git@duncanc.co.uk",
        "homepage": "https://github.com/duncan3dc",
        "role": "Developer"
    }],
    "require": {
        "facebook/webdriver": "^1.0",
        "laravel/dusk": ">=3.0.0,<3.1",
        "phpunit/phpunit": "^7.1",
        "symfony/process": "^4.0",
        "php": "^7.1"
    },
    "require-dev": {
        "duncan3dc/object-intruder": "^0.3.0",
        "maglnet/composer-require-checker": "^1.1",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {"duncan3dc\\Laravel\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"duncan3dc\\LaravelTests\\": "tests/"}
    }
}
